<html class="no-js"> <!--<![endif]-->
<head>
	<meta charset="utf-8">
	<meta http-equiv="X-UA-Compatible" content="IE=edge">
	<title>{{ page['meta']['title'] }}</title>
	<meta name="description" content="">
	<meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no">

	<!-- Place favicon.ico and apple-touch-icon.png in the root directory -->



	<script src="/vendor/components/modernizr/modernizr.{{ _v }}.js"></script>
</head>
<body>
<div class="color-line"></div>
<div>
	{% include "_header.twig" ignore missing %}

	{% include page['template'] %}

	{% include "_footer.twig" ignore missing  %}

</div>


<link rel="stylesheet" href="./ui/style.{{ _v }}.css">
<link rel="stylesheet" href="./ui/front/_style.{{ _v }}.css">
{% for row in page['css'] %}
	<link rel="stylesheet" href="{{ row }}">
{% endfor %}
{% if page['template_css'] %}
	<link rel="stylesheet" href="{{ page['template_css'] }}">
{% endif %}
<script src="/ui/javascript.{{ _v }}.js"></script>


{% for row in page['js'] %}
	<script src="{{ row }}"></script>
{% endfor %}
{% if page['template_js'] %}
	<script src="{{ page['template_js'] }}"></script>
{% endif %}










{% if page['template_jtmpl'] %}
	{% include page['template_jtmpl'] %}
{% endif %}

{% include 'template/template.jtmpl' %}



<div class="modal fade" id="systemTimers-container" tabindex="-1" role="dialog" aria-labelledby="SystemTimers" aria-hidden="true">
	<div class="modal-dialog">
		<div class="modal-content">
			<div class="modal-header">
				<button type="button" class="close" data-dismiss="modal" aria-label="Close">
					<span aria-hidden="true">&times;</span></button>
				<h4 class="modal-title" id="myModalLabel">Debug</h4>
			</div>
			<div class="modal-body" style="padding:0;">
				<article class="table-responsive">
					<table id='systemTimers' class="table font-small">

					</table>
				</article>
			</div>
		</div>
	</div>
</div>


<div class="modal fade" id="modal-window" role="dialog" aria-labelledby="modal-window" aria-hidden="true"> </div>

