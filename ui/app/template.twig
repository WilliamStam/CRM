<!doctype html>
<html class="no-js" lang="">
<head>
	<meta charset="utf-8">
	<meta http-equiv="x-ua-compatible" content="ie=edge">
	<title>{{ page['meta']['title'] }}</title>
	<meta name="description" content="">
	<meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no">
	

	<script src="/vendor/components/modernizr/modernizr.{{ _v }}.js"></script>
</head>
<body id="template-page-{{ page['id'] }}" >

	{% include "_header.twig" ignore missing %}
	
	
	
	<section class="container" id="page-content">
		{% include page['template'] %}
	</section>
	
	{% include "_footer.twig" ignore missing  %}

	<!--<link href="https://fonts.googleapis.com/css?family=Open+Sans:300,400,600,700" rel="stylesheet">-->


	<link rel="stylesheet" href="/ui/style.{{ _v }}.css">
	<link rel="stylesheet" href="/ui/app/_style.{{ _v }}.css">
	{% for row in page['css'] %}
		<link rel="stylesheet" href="{{ row }}">
	{% endfor %}
	{% if page['template_css'] %}
		<link rel="stylesheet" href="{{ page['template_css'] }}">
	{% endif %}
	<style type="text/css">
		{% for item in _css %}
		{{ item|raw() }};
		{% endfor %}
	</style>


<script src="/ui/javascript.{{ _v }}.js"></script>
<script src="/ui/app/_javascript.{{ _v }}.js"></script>


{% for row in page['js'] %}
	<script src="{{ row }}" defer></script>
{% endfor %}
{% if page['template_js'] %}
	<script src="{{ page['template_js'] }}" defer></script>
{% endif %}
	
	
	<script type="text/javascript">
		{% for item in _javascript %}
		{{ item|raw() }};
		{% endfor %}
	</script>





{% if page['template_jtmpl'] %}
	{% include page['template_jtmpl'] %}
{% endif %}

{% include 'template/template.jtmpl' %}



<div class="modal fade" id="systemTimers-container" tabindex="-1" role="dialog" aria-labelledby="SystemTimers" aria-hidden="true">
	<div class="modal-dialog">
		<div class="modal-content">
			<div class="modal-header">
				<button type="button" class="close" data-dismiss="modal" aria-label="Close">
					<span aria-hidden="true">&times;</span></button>
				<h4 class="modal-title" id="myModalLabel">Debug</h4>
			</div>
			<div class="modal-body" style="padding:0;">
				<article class="table-responsive">
					<table id='systemTimers' class="table font-small">
					
					</table>
				</article>
			</div>
		</div>
	</div>
</div>


<div class="modal fade" id="form-modal" role="dialog" aria-labelledby="form-modal" aria-hidden="true"> </div>
<div class="modal fade" id="modal-window" role="dialog" aria-labelledby="modal-window" aria-hidden="true"> </div>



<div class="loadingmask fa" id="loadingmask" >
	<i class="fa fa-cog fa-spin"></i>
</div>
	
	
	<div id="system-timer-btn" >
		<a href="#systemTimers-container" data-toggle="modal"><i class="fa fa-clock-o"></i></a>
	</div>
